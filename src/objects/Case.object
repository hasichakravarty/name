    <validationRules>
        <fullName>Adjustment_Type_Registr_MandatoryField</fullName>
        <active>true</active>
        <description>For Adjustment Type = Revenue --&gt; the Requested Dollar Value should be required 
For Adjustment Type = Registration --&gt; the Requested Unit Count should be required</description>
        <errorConditionFormula>RecordType.Name = &quot;Finance Workflow&quot; &amp;&amp;
ISPICKVAL( Adjustment_Type__c, &quot;Registration&quot;) &amp;&amp;  ISBLANK(Requested_Unit_Count__c)
&amp;&amp;  !$Setup.Bypass_Settings__c.Disable_Validation_Rules__c</errorConditionFormula>
        <errorDisplayField>Requested_Unit_Count__c</errorDisplayField>
        <errorMessage>For Registration Adjustment Type, please enter Requested Unit Count.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Adjustment_Type_Revenue_MandatoryField</fullName>
        <active>true</active>
        <description>For Adjustment Type = Revenue --&gt; the Requested Dollar Value should be required</description>
        <errorConditionFormula>RecordType.Name = &quot;Finance Workflow&quot; &amp;&amp;
ISPICKVAL( Adjustment_Type__c, &quot;Revenue&quot;) &amp;&amp;  ISBLANK(Requested_Dollar_Value__c)
&amp;&amp;  !$Setup.Bypass_Settings__c.Disable_Validation_Rules__c</errorConditionFormula>
        <errorDisplayField>Requested_Dollar_Value__c</errorDisplayField>
        <errorMessage>For Revenue Adjustment Type, please enter Requested Dollar Value.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Appeal_cannot_be_selected</fullName>
        <active>true</active>
        <description>Appeal check box can only be selected by Regional Manager and above</description>
        <errorConditionFormula>AND(Appeal__c,NOT(CONTAINS($UserRole.Name, &apos;Regional Sales Manager&apos;) ) , NOT(CONTAINS($UserRole.Name, &apos;Sales General Manager&apos;) ), NOT(ISPICKVAL(Status, &apos;Close-Declined&apos;)),RecordType.Name = &apos;Finance Workflow&apos;)</errorConditionFormula>
        <errorMessage>Only declined cases can be appealed by Regional Managers and above.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Bursay_Request_Close_case</fullName>
        <active>true</active>
        <description>the user must update the approval status field to either - &apos;Approved&apos; or &apos;Rejected&apos; before closing the case.</description>
        <errorConditionFormula>AND
(
$RecordType.Name ==$Label.Loan_Bursary_Request,
ISPICKVAL( Approval_Status__c,&apos;Submitted&apos;),
ISPICKVAL( Status,&apos;Closed&apos;),
$Setup.Bypass_Settings__c.Disable_Validation_Rules__c==false
)</errorConditionFormula>
        <errorMessage>Please update picklist Approval Status to &quot;Approved&quot; or &quot;Rejected&quot; before closing the case.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Campus_Fields_cannot_be_blank</fullName>
        <active>true</active>
        <description>This validation rule will ensure that, when the selected type is Change Request , then Current Campus, Proposed campus must be populated.</description>
        <errorConditionFormula>$RecordType.Name=$Label.Change_Request_Record_Type  &amp;&amp; ISPICKVAL(Type, &apos;Change Campus&apos;) &amp;&amp;
(
    ISPICKVAL(Current_Campus__c,&apos;&apos;) ||
    ISPICKVAL(Proposed_Campus__c, &apos;&apos;) 
)
&amp;&amp;
!$Setup.Bypass_Settings__c.Disable_Validation_Rules__c</errorConditionFormula>
        <errorMessage>Please enter the Student&apos;s&apos; Current Campus&apos; and &apos;Proposed Campus&apos; before submitting the Campus Transfer Request</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Case_Approval_Status</fullName>
        <active>true</active>
        <description>If ‘Approval Status’ = Approved, then ‘Approved Amount’ must be populated</description>
        <errorConditionFormula>AND(ISPICKVAL(Approval_Status__c, &apos;Approved&apos;),
ISBLANK( Approved_amount__c )
)
&amp;&amp; 
($RecordType.Name== $Label.Cancellation_Request_Record_type ||$RecordType.Name== $Label.Case_custom_label||$RecordType.Name== $Label.Change_Request_Record_Type)
&amp;&amp; 
NOT($Setup.Bypass_Settings__c.Disable_Validation_Rules__c)</errorConditionFormula>
        <errorMessage>If ‘Approval Status’ = Approved, then ‘Approved Amount’ must be populated</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Case_Bursary_Loan</fullName>
        <active>true</active>
        <description>If ‘Approved/Rejected By’ = Other, then the ‘Approved/Rejected by (if Other)’ must be populated</description>
        <errorConditionFormula>AND(ISPICKVAL(Institution__c ,&apos;other&apos;),
ISBLANK (Approved_by_if_Other__c )
)
&amp;&amp; 
($RecordType.Name== $Label.Cancellation_Request_Record_type ||$RecordType.Name== $Label.Case_custom_label||$RecordType.Name== $Label.Change_Request_Record_Type)
&amp;&amp;
NOT($Setup.Bypass_Settings__c.Disable_Validation_Rules__c)</errorConditionFormula>
        <errorMessage>If ‘Approved/Rejected By’ is Other, then the ‘Approved/Rejected by (if Other)’ must be populated</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Case_Reason_for_Other_is_Required</fullName>
        <active>true</active>
        <description>Case Reason for &apos;Other&apos; is Required</description>
        <errorConditionFormula>AND(RecordType.Name=&apos;Cancellation Request&apos;, ISPICKVAL( Reason , &apos;Other&apos;), ISBLANK( Reason_if_Other__c ),NOT($Setup.Bypass_Settings__c.Disable_Validation_Rules__c) )</errorConditionFormula>
        <errorDisplayField>Reason_if_Other__c</errorDisplayField>
        <errorMessage>Please provide a case reason</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Contact_is_Mandatory</fullName>
        <active>true</active>
        <description>Student Contact is Mandatory</description>
        <errorConditionFormula>AND ( $RecordType.Name=$Label.Cancellation_Request_Record_type,OR( ISBLANK( ContactId ), ISNULL( ContactId )),
NOT($Setup.Bypass_Settings__c.Disable_Validation_Rules__c))</errorConditionFormula>
        <errorMessage>Contact is Mandatory</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Either_Product_or_ISBN_Must_be_Populated</fullName>
        <active>true</active>
        <errorConditionFormula>RecordType.Name = &apos;Finance Workflow&apos; &amp;&amp;
ISBLANK(Product__c) &amp;&amp; ISBLANK(ISBN__c)</errorConditionFormula>
        <errorMessage>Please enter either product information or ISBN information before saving the case.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Email_is_Mandatory_For_Student_Contact</fullName>
        <active>true</active>
        <description>Student (Contact) should have primary or secondary email</description>
        <errorConditionFormula>AND( 
AND(ISblank(Contact.Email),
    ISblank(Contact.Secondary_Email__c)
    ),
(OR($RecordType.Name=$Label.Cancellation_Request_Record_type    ) 
),
NOT($Setup.Bypass_Settings__c.Disable_Validation_Rules__c)
)</errorConditionFormula>
        <errorMessage>Student (Contact) should have primary or secondary email so that an automated email can be sent</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Laptop_to_be_returned</fullName>
        <active>false</active>
        <description>Case Creator must specify whether the student is returning their tablet. This information will be used by Finance to calculate whether outstanding payment is due from the student</description>
        <errorConditionFormula>AND (
 $RecordType.Name=$Label.Change_Request_Record_Type,

ISPICKVAL(  Returning_Tablet__c ,&apos;&apos;)
)</errorConditionFormula>
        <errorMessage>Please specify whether the student has returned their tablet</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Preference_is_mandatory</fullName>
        <active>true</active>
        <description>Used to ensure that a students accommodation preferences are specified</description>
        <errorConditionFormula>AND
($RecordType.Name=$Label.Change_Request_Record_Type,  ISPICKVAL( Type , &quot;Change Accommodation&quot;), ISPICKVAL( Preferences__c , &quot;&quot;),NOT($Setup.Bypass_Settings__c.Disable_Validation_Rules__c) )</errorConditionFormula>
        <errorDisplayField>Preferences__c</errorDisplayField>
        <errorMessage>Please specify the students accommodation preference</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Product_In_Use_to_be_populated</fullName>
        <active>true</active>
        <description>Used to ensure that if the Campus Transfer affects an active enrollment, that the enrollment or &apos;Product In Use&apos; is referenced on the case</description>
        <errorConditionFormula>AND
(OR($RecordType.Name=$Label.Change_Request_Record_Type,
$RecordType.Name=$Label.Cancellation_Request_Record_type),
ISPICKVAL( Type , &quot;Change Campus&quot;), 
ISPICKVAL(  Post_Enrolment__c  , &quot;Yes&quot;),  
ISBLANK( Products_in_Use__c ),NOT($Setup.Bypass_Settings__c.Disable_Validation_Rules__c)  )</errorConditionFormula>
        <errorMessage>Please populate the Students Product In Use (Qualification)</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Request_BursaryLoan</fullName>
        <active>true</active>
        <description>If the ‘Status’ is updated to closed, the &quot;Approval Status&quot; must be populated
If the ‘Approval Status’ = &quot;Approved/Rejected&quot;, then ‘Approved/Rejected By’ must be populated</description>
        <errorConditionFormula>AND(
 $RecordType.Name=$Label.Loan_Bursary_Request,
OR(and(ISPICKVAL( Status,&apos;closed&apos;),ISPICKVAL (Approval_Status__c ,&apos;&apos;)),and(OR(ISPICKVAL(Approval_Status__c, &apos;Approved&apos;),ISPICKVAL(Approval_Status__c,&apos;Rejected&apos;)),ISPICKVAL(Institution__c,&apos;&apos;))),
NOT($Setup.Bypass_Settings__c.Disable_Validation_Rules__c) )</errorConditionFormula>
        <errorMessage>If the ‘Status’ is updated to &quot;closed&quot;, the &quot;Approval Status&quot; must be populated
If the ‘Approval Status’ is &quot; Approved/Rejected&quot;, then ‘Approved/Rejected By’ must be populated</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Sponsor_Email_is_Mandatory</fullName>
        <active>true</active>
        <description>If Type = Change Sponsor, Sponsor field must not be blank</description>
        <errorConditionFormula>(TEXT(Type) == &apos;Change Sponsor&apos; &amp;&amp;  ISBLANK(Sponsor_name__c))&amp;&amp; ($RecordType.Name== $Label.Cancellation_Request_Record_type ||$RecordType.Name== $Label.Case_custom_label||$RecordType.Name== $Label.Change_Request_Record_Type)
&amp;&amp;
NOT($Setup.Bypass_Settings__c.Disable_Validation_Rules__c)</errorConditionFormula>
        <errorMessage>Please provide the name of the Proposed Sponsor in the &apos;Sponsor Name&apos; field</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Sponsor_is_Mandatory</fullName>
        <active>true</active>
        <description>Student Sponsor is Mandatory</description>
        <errorConditionFormula>AND (  OR($RecordType.Name=$Label.Cancellation_Request_Record_type ,
          AND(RecordType.Name == &apos;Change Request&apos; ,  ISPICKVAL(Type, &apos;Change Sponsor&apos;))),
       OR( ISBLANK(   Sponsor_name__c  ), ISNULL(  Sponsor_name__c )),
NOT($Setup.Bypass_Settings__c.Disable_Validation_Rules__c)
    )</errorConditionFormula>
        <errorMessage>Sponsor  is Mandatory</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Status_cant_be_changed</fullName>
        <active>true</active>
        <description>Doesn&apos;t allow user to change the status field directly from the UI</description>
        <errorConditionFormula>OR ( (ISCHANGED(Status)) , 
(ISNEW() &amp;&amp; !ISPICKVAL(Status,&quot;New&quot;)))
&amp;&amp; RecordType.Name = &quot;Finance Workflow&quot; &amp;&amp; !$Setup.Bypass_Settings__c.Disable_Validation_Rules__c</errorConditionFormula>
        <errorMessage>You can&apos;t change the status field without sending this case for approval. Please also make sure the status of a new case is &quot;New&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SubType_is_mandatory_Accom_Request</fullName>
        <active>true</active>
        <description>Used to ensure the type of Accommodation Request is specified</description>
        <errorConditionFormula>AND
 ( $RecordType.Name=$Label.Change_Request_Record_Type,ISPICKVAL( Type , &quot;Change Accommodation&quot;),  ISPICKVAL(  Request_Sub_Type__c , &quot;&quot;), NOT($Setup.Bypass_Settings__c.Disable_Validation_Rules__c))</errorConditionFormula>
        <errorMessage>Please specify the &apos;Sub-Type&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Tablet_Unique_Number_is_Required</fullName>
        <active>true</active>
        <description>When the picklist field equals &quot;Yes&quot; for &quot;Tablet Returned&quot;  , &quot;Tablet Unique Number&quot; should be given</description>
        <errorConditionFormula>AND(ISBLANK( Tablet_Unique_Number__c ) ,ISPICKVAL( Returning_Tablet__c , &apos;Yes&apos;))&amp;&amp; ($RecordType.Name== $Label.Cancellation_Request_Record_type ||$RecordType.Name== $Label.Case_custom_label||$RecordType.Name== $Label.Change_Request_Record_Type)
&amp;&amp;
NOT($Setup.Bypass_Settings__c.Disable_Validation_Rules__c)</errorConditionFormula>
        <errorMessage>Please Populate Tablet Unique Number</errorMessage>
    </validationRules>
</CustomObject>
