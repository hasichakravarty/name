    <validationRules>
        <fullName>Academic_Achiever_level_req_for_Schools</fullName>
        <active>true</active>
        <description>For Organisation Account rececords with the type school the Academic Achiever level is required.</description>
        <errorConditionFormula>Market__c=&apos;ZA&apos;  
&amp;&amp;  
 RecordType.DeveloperName==&apos;Organisation&apos;
&amp;&amp;  
ISPICKVAL(Academic_Achievement__c,&apos;&apos;)
&amp;&amp;
ISPICKVAL( Organisation_Type__c,&apos;School&apos;)
&amp;&amp;
NOT($Setup.Bypass_Settings__c.Disable_Validation_Rules__c)</errorConditionFormula>
        <errorDisplayField>Academic_Achievement__c</errorDisplayField>
        <errorMessage>A Value must be provided for the Academic Achiever level for School Organisation types.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Account_New_Bookmaster_Account_Request</fullName>
        <active>true</active>
        <description>CORE - On a New Account type when the user check the &quot;Create Bookmaster Account&quot; there is a minimum set of data required.</description>
        <errorConditionFormula>Create_Bookmaster_Account__c = True
&amp;&amp;
OR(
ISBLANK(  BillingStreet  ),
ISBLANK(  BillingCity  ),
ISBLANK(  BillingPostalCode  ),
OR(ISBLANK(BillingCountry),AND(BillingCountry==&quot;Australia&quot;,ISBLANK(BillingState))),
OR(ISBLANK(ShippingCountry),AND(ShippingCountry==&quot;Australia&quot;,ISBLANK(ShippingState))),
ISBLANK(  ShippingStreet  ),
ISBLANK(  ShippingCity  ),
ISBLANK(  ShippingPostalCode  ),
ISPICKVAL(Category__c, &quot;&quot;),
ISBLANK( Email__c ),
ISBLANK( Phone )
)</errorConditionFormula>
        <errorMessage>When requesting a Bookmaster Account to be Created, the following Account fields must be populated at a minimum:
Shipping Address,
Billing Address,
Category,
Phone,
Email.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Fax_Validation_Rule</fullName>
        <active>true</active>
        <description>A validation rule to ensure that only certain characters are allowed to be populated in the Fax field.</description>
        <errorConditionFormula>IF( 
ISBLANK( Fax ) , false ,NOT(REGEX( Fax , &quot;[0-9+\\-\\(\\)\\&apos; &apos;]*&quot;))) 
&amp;&amp;(($RecordType.Name==$Label.PS_Account_Organisation)||RecordType.Name =&apos;Learner&apos;)
&amp;&amp; ($Setup.Bypass_Settings__c.Disable_Validation_Rules__c==false)</errorConditionFormula>
        <errorMessage>Please enter a valid fax number containing only these characters: 0-9, +, -, ()</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Learner_Account_Name_is_Read_Only</fullName>
        <active>true</active>
        <description>Global: Used to Ensure that Account Name on a Learner Account is only updated via the Primary Contact</description>
        <errorConditionFormula>AND( 
RecordType.Name =&apos;Learner&apos;, 
Account_Name_is_Read_only__c == true, 
OR( ISCHANGED( Name ),ISCHANGED(Account_Surname__c)),
NOT($Setup.Bypass_Settings__c.Disable_Validation_Rules__c))</errorConditionFormula>
        <errorMessage>To change the Account Name/Account Surname, Please update the First Name/Last Name on the Student&apos;s Contact record ( Contact with Primary Checkbox)</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PS_Physical_Address_Mandatory</fullName>
        <active>true</active>
        <description>To make physical address mandatory</description>
        <errorConditionFormula>And( IsCreatedFromLead__c==False, $RecordType.Name==&apos;Organisation&apos;,OR(ISBLANK(ShippingCountry), 
ISBLANK( ShippingCity ),ISBLANK( ShippingStreet   ),ISBLANK(  ShippingPostalCode )), NOT($Setup.Bypass_Settings__c.Disable_Validation_Rules__c))</errorConditionFormula>
        <errorMessage>Please enter details at Physical address</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PS_Shipping_Address_Fields_are_mandatory</fullName>
        <active>false</active>
        <description>Pearson Shipping_Address_Fields_are_mandatory</description>
        <errorConditionFormula>AND(RecordType.Name==&apos;Organisation&apos;, OR(ISBLANK( ShippingCity ),ISBLANK(ShippingCountry)))&amp;&amp; NOT( $Setup.Bypass_Settings__c.Disable_Validation_Rules__c &amp;&amp; $RecordType.Name!=$Label.ANZ_Record_Type)</errorConditionFormula>
        <errorMessage>Physical Address is required.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Pearson_Restriction_from_Record_Creation</fullName>
        <active>true</active>
        <description>Users with profile &apos;Pearson Sales User Official&apos; are restricted from creating Learner accounts except through lead conversion</description>
        <errorConditionFormula>AND(ISNEW(),
NOT(IsCreatedFromLead__c),
(RecordType.Name ==&apos;Learner&apos;), 
($Profile.Name==&apos;Pearson Sales User OneCRM&apos;),
NOT($Setup.Bypass_Settings__c.Disable_Validation_Rules__c))</errorConditionFormula>
        <errorMessage>You can only create a Learner Account by Converting a Lead</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Phone_number_validation</fullName>
        <active>true</active>
        <errorConditionFormula>IF(
ISBLANK(Phone) , false ,NOT(REGEX(Phone, &quot;[0-9+\\-\\(\\)\\&apos; &apos;]*&quot;)))
&amp;&amp; 
(($RecordType.Name==$Label.PS_Account_Organisation)||RecordType.Name =&apos;Learner&apos;)
&amp;&amp; ($Setup.Bypass_Settings__c.Disable_Validation_Rules__c==false)</errorConditionFormula>
        <errorMessage>Please enter a valid phone number containing only these characters: 0-9, +, -, ()</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Prevent_Change_of_Account_name</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL($User.Market__c , &quot;US&quot;) , $Profile.Name == &quot;Pearson Sales User OneCRM&quot; , ischanged( Name ))</errorConditionFormula>
        <errorMessage>You do not have the ability to change the Account Name. Please contact your administrator for this request.</errorMessage>
    </validationRules>
    <validationRules>
