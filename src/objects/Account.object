        <fullName>Bookmaster_Verified__c</fullName>
        <description>CORE - Formula field to visually flag if an account is in bookmaster or not.</description>
        <externalId>false</externalId>
        <formula>IF (
   AND(Create_Bookmaster_Account__c = False, 
         OR(ISBLANK(External_Account_Number__c), 
            BEGINS(External_Account_Number__c, &apos;a&apos;), 
            BEGINS(External_Account_Number__c, &apos;n&apos;))),
IMAGE(&quot;/img/msg_icons/error32.png&quot;,&quot;Red&quot;),
IF (AND(Create_Bookmaster_Account__c = True, ISBLANK(External_Account_Number__c)), 
IMAGE(&quot;/img/msg_icons/warning32.png&quot;,&quot;Orange&quot;),
IMAGE(&quot;/img/msg_icons/confirm32.png&quot;,&quot;Green&quot;) ))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Bookmaster Verified</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <type>Text</type>
        <unique>false</unique>
