    <validationRules>
        <fullName>Adjustment_Type_Registr_MandatoryField</fullName>
        <active>true</active>
        <description>For Adjustment Type = Revenue --&gt; the Requested Dollar Value should be required 
For Adjustment Type = Registration --&gt; the Requested Unit Count should be required</description>
        <errorConditionFormula>RecordType.Name = &quot;Finance Workflow&quot; &amp;&amp;
ISPICKVAL( Adjustment_Type__c, &quot;Registration&quot;) &amp;&amp; ISBLANK(Requested_Unit_Count__c)
&amp;&amp; !$Setup.Bypass_Settings__c.Disable_Validation_Rules__c</errorConditionFormula>
        <errorDisplayField>Requested_Unit_Count__c</errorDisplayField>
        <errorMessage>For Registration Adjustment Type, please enter Requested Unit Count.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Adjustment_Type_Revenue_MandatoryField</fullName>
        <active>true</active>
        <description>For Adjustment Type = Revenue --&gt; the Requested Dollar Value should be required</description>
        <errorConditionFormula>RecordType.Name = &quot;Finance Workflow&quot; &amp;&amp;
ISPICKVAL( Adjustment_Type__c, &quot;Revenue&quot;) &amp;&amp;  ISBLANK(Requested_Dollar_Value__c)
&amp;&amp;  !$Setup.Bypass_Settings__c.Disable_Validation_Rules__c</errorConditionFormula>
        <errorDisplayField>Requested_Dollar_Value__c</errorDisplayField>
        <errorMessage>For Revenue Adjustment Type, please enter Requested Dollar Value.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Appeal_cannot_be_selected</fullName>
        <active>true</active>
        <description>Appeal check box can only be selected by Regional Manager and above</description>
        <errorConditionFormula>AND(Appeal__c,NOT(CONTAINS($UserRole.Name, &apos;Regional Sales Manager&apos;) ) , NOT(CONTAINS($UserRole.Name, &apos;Sales General Manager&apos;) ), NOT(ISPICKVAL( Status__c , &apos;Close-Declined&apos;)),RecordType.Name = &apos;Finance Workflow&apos;)</errorConditionFormula>
        <errorMessage>Only declined cases can be appealed by Regional Managers and above.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Either_Product_or_ISBN_Must_be_Populated</fullName>
        <active>true</active>
        <errorConditionFormula>RecordType.Name = &apos;Finance Workflow&apos; &amp;&amp;
ISBLANK(Product__c) &amp;&amp; ISBLANK(ISBN__c)</errorConditionFormula>
        <errorMessage>Please enter either product information or ISBN information before saving the case.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Expected_Change_Date</fullName>
        <active>true</active>
        <description>This validation rule requires the record creator to enter a date greater than the system date at the time of creation.</description>
        <errorConditionFormula>Expected_Realignment_Date__c &lt;  TODAY()</errorConditionFormula>
        <errorMessage>An internal request record cannot be submitted for a date in the past, please enter a proper expected change date (i.e. today&apos;s date or greater)</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Status_cant_be_changed</fullName>
        <active>true</active>
        <description>Doesn&apos;t allow user to change the status field directly from the UI</description>
        <errorConditionFormula>OR ( (ISCHANGED( Status__c )) , 
(ISNEW() &amp;&amp; !ISPICKVAL( Status__c ,&quot;New&quot;)))
&amp;&amp; RecordType.Name = &quot;Finance Workflow&quot; &amp;&amp; !$Setup.Bypass_Settings__c.Disable_Validation_Rules__c</errorConditionFormula>
        <errorMessage>You can&apos;t change the status field without sending this case for approval. Please also make sure the status of a new case is &quot;New&quot;</errorMessage>
    </validationRules>
