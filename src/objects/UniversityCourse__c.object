    <validationRules>
        <fullName>Enrollments_cant_be_negative</fullName>
        <active>true</active>
        <description>Enrollment Values cannot be negative values</description>
        <errorConditionFormula>(Fall_Enrollment__c &lt; 0) || (Winter_Enrollment__c &lt; 0) || (Summer_Enrollment__c &lt; 0) || ( Spring_Enrollment__c &lt;0)</errorConditionFormula>
        <errorMessage>Enrollment value cannot be a negative value.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VR_HEUK_PreventSameUnivCourseToAccount</fullName>
        <active>true</active>
        <description>Prevent creation of the university code more than once against the account.</description>
        <errorConditionFormula>!$Setup.Bypass_Settings__c.Disable_Validation_Rules__c &amp;&amp; VLOOKUP( $ObjectType.UniversityCourse__c.Fields.UniversityCourseAccountComboStrId__c , $ObjectType.UniversityCourse__c.Fields.Name , Name + &apos;-&apos; + Account__c) = Name + &apos;-&apos; + left(Account__c,15)</errorConditionFormula>
        <errorDisplayField>Name</errorDisplayField>
        <errorMessage>University course already exists against the account.</errorMessage>
    </validationRules>
