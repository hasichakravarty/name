    <validationRules>
        <fullName>Order_Approval_Only_If_Active_Products</fullName>
        <active>true</active>
        <errorConditionFormula>OR( 
AND(
LineItemCount__c==0, 
ISPICKVAL(Status__c,&apos;Approved&apos;)
),
AND( 
Inactive_Products__c &gt; 0, 
ISPICKVAL(Status__c,&apos;Approved&apos;) 
) 
)</errorConditionFormula>
        <errorMessage>Please ensure an active product has been added to the order, and all inactive products removed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Order_Approval_only_if_BookmasterAccount</fullName>
        <active>true</active>
        <description>CORE - The order can only be approved if the Account is in Bookmaster.</description>
        <errorConditionFormula>IF(
      AND(
            ISPICKVAL(TargetSystem__c,&apos;HED&apos;),
            ISPICKVAL(OrderType__c,&apos;Sample&apos;),
            OR(
                  Account__r.RecordType.Name = &apos;Higher Education&apos;,
                  Account__r.RecordType.Name = &apos;Non Integrated&apos;
            )
      ),
      FALSE,
      AND(
            OR (ISBLANK(Account__r.External_Account_Number__c), Account__r.Account_Status__c != &apos;A&apos;),
            ISPICKVAL(Status__c,&apos;Approved&apos;),
            OR(
                  Account__r.BillingCountry == &apos;Australia&apos;,
                  Account__r.BillingCountry == &apos;New Zealand&apos;
            )
      )
)</errorConditionFormula>
        <errorDisplayField>Account__c</errorDisplayField>
        <errorMessage>The Account must have a Bookmaster Account Number and be Active to have an Approved Order. Please ensure the Account is in Bookmaster and the Account Status is A.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Order_NO_Approval_HE_Non_Samples</fullName>
        <active>true</active>
        <errorConditionFormula>AND (
ISPICKVAL(Status__c, &apos;Approved&apos;) , 
ISPICKVAL (TargetSystem__c, &apos;HED&apos;), 
ISPICKVAL (OrderType__c, &apos;Normal Sale&apos;)
)</errorConditionFormula>
        <errorMessage>An HE Normal Sale Order cannot be set to &apos;Approved&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Order_PO_Number_To_Long</fullName>
        <active>true</active>
        <description>Order PO Number can not exceed 30 chars</description>
        <errorConditionFormula>AND(NOT(ISBLANK( PONumber__c )), LEN( PONumber__c ) &gt; 30)</errorConditionFormula>
        <errorDisplayField>PONumber__c</errorDisplayField>
        <errorMessage>The Purchase Order Number can not exceed 30 characters</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Required_Address_Override_Fields</fullName>
        <active>true</active>
        <description>Ensure that override Address Line 1, City, State, Country and Zip are populated if Address Override = true</description>
        <errorConditionFormula>AND(
	OverrideDeliverToAddress__c,
	OR(
		ISBLANK(OverrideDeliverToAddressLine1__c),
		ISBLANK(OverrideDeliverToCity__c),
		ISBLANK(OverrideDeliverToCountry__c),
		ISBLANK(OverrideDeliverToState__c),
		ISBLANK(OverrideDeliverToZipCode__c)
	)
)</errorConditionFormula>
        <errorMessage>If choosing to override the deliver to address, please populate the Override Deliver Address Line 1, Override Deliver to City, Override Deliver to Country, Override Deliver to State and Override Deliver to Zip Code fields.</errorMessage>
    </validationRules>
