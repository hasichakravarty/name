        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/28.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/28.0/apex.js&quot;)} 

function popupErrorAlert() { 
var alertString=&quot;IMPORTANT NOTICE: \nAn Opportunity must be Closed and must have products associated before you can convert products to products in use.&quot;; 
alert(alertString); 
} 

function convertedProductErrorAlert() { 
var alertStr = &quot;The Products in Use records have already been created for this Closed Opportunity. Please create any additional Products in Use directly on the associated Course.&quot;; 
alert(alertStr); 
} 

// Redirects to VisualForce Page with Opportunity ID and Account ID as Query String parameters 
function redirect() { 
var callback = { onSuccess: function(result){alert(&apos;success&apos;)}, onFailure: function(error){alert(&apos;fail&apos;)} }; 
var soptyId=&quot;{!Opportunity.Id}&quot;; 
var soptyAccId=&quot;{!Opportunity.AccountId}&quot;; 
sforce.apex.execute(&quot;OppProductToAsset&quot;,&quot;convertToAsset&quot;,{oppId:soptyId,accId:soptyAccId}); 
var ocr = sforce.connection.query(&quot;SELECT UniversityCourse__c from OpportunityUniversityCourse__c where Opportunity__c = &apos;{!Opportunity.Id}&apos;&quot;); 
var records = ocr.getArray(&apos;records&apos;); 

if(records.length &gt; 0){	
location.href=&quot;/apex/BulkContactPIUEditPage?courseId=&quot;+records[0].UniversityCourse__c; 
} 
else{ 
location.href=&quot;/{!Opportunity.AccountId}&quot;; 
} 
} 
try{ 

if((&quot;{!Opportunity.StageName}&quot; === &quot;Closed&quot;) &amp;&amp; {!Opportunity.Product_Count__c} &gt; 0) { 
if({!Opportunity.Product_Converted__c == false}) { 
redirect(); 
} 
else{ 
convertedProductErrorAlert(); 
} 
} 
else { 
popupErrorAlert(); 
} 
} 
catch(e){ 
alert(e); 
}</url>
