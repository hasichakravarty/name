	  AND(
	    IsClosed,

	    (Year(Renewal_Date__c)*12 + Month(Renewal_Date__c) - 
	    (Year(Re_engagement_Date__c)*12 + Month(Re_engagement_Date__c)) + IF(DAY(Re_engagement_Date__c) &lt;= DAY(Renewal_Date__c), 1, 0) &lt; 4)
