    <validationRules>
        <fullName>Integer_value_for_Quantity</fullName>
        <active>true</active>
        <errorConditionFormula>FLOOR(Quantity  ) != Quantity &amp;&amp;  !($Setup.Bypass_Settings__c.Disable_Validation_Rules__c)</errorConditionFormula>
        <errorMessage>Please enter numbers without decimals in the Quantity  field.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Limit_on_Quantity_than_can_be_entered</fullName>
        <active>true</active>
        <description>This is to prevent the setting of value of Quantity grater than 500.</description>
        <errorConditionFormula>Quantity  &gt; 500 &amp;&amp; !( $Setup.Bypass_Settings__c.Disable_Validation_Rules__c )</errorConditionFormula>
        <errorMessage>You cannot enter a quantity greater than 500</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Prevent_Change_of_Record</fullName>
        <active>false</active>
        <description>This is used to prevent change of record field values except for Status Field</description>
        <errorConditionFormula>AND(!ISNEW() ,OR(ISCHANGED( Product2Id ),ISCHANGED( Quantity ),ISCHANGED(  OrderId ),ISCHANGED(  UnitPrice),ISCHANGED(  ERP_Order_Number__c),ISCHANGED(ISBN13__c),ISCHANGED(StatusReason__c),ISCHANGED(RequestShipMethodCode__c),ISCHANGED(ForceShipIndicator__c),ISCHANGED(RestrictionOverrideIndicator__c),ISCHANGED(ForceShipIndicator__c),ISCHANGED(DuplicateOverrideIndicator__c),ISCHANGED(TitleStatusOverrideIndicator__c),ISCHANGED(Description),ISCHANGED(Target_System__c)))</errorConditionFormula>
        <errorMessage>You are only allowed to change the Status Field</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Test</fullName>
        <active>false</active>
        <errorConditionFormula>1==1</errorConditionFormula>
        <errorDisplayField>Quantity</errorDisplayField>
        <errorMessage>error test</errorMessage>
    </validationRules>
</CustomObject>
