    <validationRules>
        <fullName>C_PreventAccountChange</fullName>
        <active>true</active>
        <description>Validation to prevent the change of the Account__c field</description>
        <errorConditionFormula>ISCHANGED( Account__c )</errorConditionFormula>
        <errorMessage>The change of the Account is not allowed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PS_Cannot_Uncheck_Active_Flag</fullName>
        <active>true</active>
        <description>User cannot uncheck the Active flag if the Account Contact is the Financially responsible person.</description>
        <errorConditionFormula>AND( Account__r.RecordType.DeveloperName = &apos;Learner&apos;, Active__c  = False, Financially_Responsible__c  = True, $Setup.Bypass_Settings__c.Disable_Validation_Rules__c==false)</errorConditionFormula>
        <errorMessage>You cannot uncheck the Active flag if the Account Contact is a Financially Responsible person. If you would like to uncheck the Active flag for this person update another Account Contact record with the Financially Responsible flag first.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RoleDetailRequiredAccountContact</fullName>
        <active>true</active>
        <description>Role detail must be chosen when role of educator is selected</description>
        <errorConditionFormula>ISPICKVAL( AccountRole__c, &quot;Educator&quot;) &amp;&amp; ISBLANK(TEXT( Role_Detail__c))</errorConditionFormula>
        <errorMessage>You must choose Role Detail when Role of Educator is chosen</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Select_Other_Academic_Role</fullName>
        <active>true</active>
        <description>Core: If Academic_Role is other, ensure the other role is populated</description>
        <errorConditionFormula>AND(ISBLANK(Other_Academic_Role__c), ISPICKVAL(Academic_Role__c, &quot;Other&quot;))</errorConditionFormula>
        <errorMessage>Please enter an Other Academic Role</errorMessage>
    </validationRules>
