        <fullName>Cannot_Change_Order_Lines_Approved</fullName>
        <active>true</active>
        <errorConditionFormula>AND (Contains ($Profile.Name , &apos;Sales&apos;), OR (ISCHANGED( ItemQuantity__c), ISCHANGED( Product__c )),  ISPICKVAL (Order__r.Status__c,&apos;Approved&apos;))</errorConditionFormula>
        <errorMessage>You cannot modify an Order Line Item when the order is Approved</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cannot_enter_discount_greater_than_100</fullName>
        <active>true</active>
        <description>Prevents a discount greater than 100% being entered.</description>
        <errorConditionFormula>Discount__c &gt;1</errorConditionFormula>
        <errorDisplayField>Discount__c</errorDisplayField>
        <errorMessage>Discount percent cannot exceed 100%</errorMessage>
    </validationRules>
